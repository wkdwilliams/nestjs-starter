{
  "name": "booking",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "&& nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node node_modules/typeorm/cli.js",
    "db:seed": "ts-node ./src/database/seeder.ts",
    "db:migrate": "npm run typeorm -- -d ./src/database/typeOrm.config.ts migration:run",
    "db:generate": "npm run typeorm -- -d ./src/database/typeOrm.config.ts migration:generate ./src/database/migrations/$npm_config_name",
    "db:create": "npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
    "db:revert": "npm run typeorm -- -d ./src/database/typeOrm.config.ts migration:revert",
    "db:show": "npm run typeorm -- migration:show -d ./src/database/typeOrm.config.ts"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.3.9",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^9.4.2",
    "argon2": "^0.30.3",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "handlebars": "^4.7.7",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.2.0",
    "nestjs-console": "^9.0.0",
    "nestjs-typeorm-paginate": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "ts-md5": "^1.3.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.3.1",
    "nestjs-seeder": "^0.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
